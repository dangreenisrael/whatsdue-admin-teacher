<section id="main">
    <!-- main content start-->
    <div class="main-content {{mobileMenu}}" >
    <!-- header section start-->
    <div class="header-section">
        <!--notification menu start -->
        <div class="pull-left">
            <img {{action "toggleMenu"}} id="logo" src="/ember/images/whatsdue-logo.png"/>
        </div>
        <div class="menu-right ">
            <ul class="notification-menu">
                <li id="invite-users">
                    <a {{action 'inviteUsers'}} class="btn btn-default dropdown-toggle" data-toggle="dropdown">
                        Invite Users <i class="fa fa-users"></i>
                    </a>
                </li>
                <li>
                    <a href="#" class="btn btn-default dropdown-toggle" data-toggle="dropdown">
                        <i class="fa fa-info-circle"></i>
                            {{user.first_name}}
                        <span class="caret"></span>
                    </a>
                    <ul class="dropdown-menu dropdown-menu-usermenu pull-right">
                        <li>
                            <a {{action 'walkthrough' 'intro'}}>
                                <i class="fa fa-envelope"></i>
                                Walkthrough
                            </a>
                        </li>
                        <li><a href="mailto:aaron@whatsdueapp.com"><i class="fa fa-envelope"></i>Email Aaron</a></li>
                        <li><a href="skype:aarontaylor613?call"><i class="fa fa-phone"></i>Skype Aaron</a></li>
                        <li><a href="/logout"><i class="fa fa-sign-out"></i> Log Out</a></li>
                    </ul>
                </li>
            </ul>
        </div>
        <!--notification menu end -->
    </div>
    <!-- header section end-->
    <!--body wrapper start-->
    <div class="wrapper">
        {{outlet "main"}}
    </div>
    <!--body wrapper end-->
    </div>
    <!-- main content end-->
</section>
<!--&lt;!&ndash;footer section start&ndash;&gt;-->
<!--<footer id="mainFooter">-->
    <!--2014 &copy; WhatsDue-->
<!--</footer>-->
<!--&lt;!&ndash;footer section end&ndash;&gt;-->

<div class="modal fade" id="Modal" tabindex="-1" role="dialog" >
    <div class="modal-dialog">
        <div class="modal-content">
            {{outlet "modal"}}
        </div>
    </div>
</div>


<!-- Tooltip walk through -->
{{outlet "tooltips"}}
