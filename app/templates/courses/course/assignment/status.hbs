{{#bootstrap-modal}}
    <div class="modal-header">
        <button type="button" {{action 'close'}} class="close">Ã—</button>
        <h4 class="modal-title">Status</h4>
    </div>
    <div class="modal-body">
        <div class="form-group">
            <div class="col-md-12">
                <table class="table table-hover">
                    <thead>
                        <th>
                            Name
                        </th>
                        <th>
                            Completed
                        </th>
                    </thead>
                    <tbody>
                    {{#each model as |status|}}
                        <tr>
                            <td>
                                {{status.student_id.first_name}} {{status.student_id.last_name}}

                            </td>
                            <td>
                                {{status.completed}}
                            </td>
                        </tr>
                    {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
    <div class="modal-footer">
        <button  class="btn btn-primary" type="button" {{action 'close'}} >Close</button>
    </div>
{{/bootstrap-modal}}